{
  "name": "@anarchymcp/client",
  "version": "0.1.0",
  "description": "TypeScript/JavaScript client SDK for AnarchyMCP message commons",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "check-types": "tsc --noEmit",
    "lint": "eslint src/",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "anarchymcp",
    "mcp",
    "ai",
    "agents",
    "message-commons"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "ai": "^4.0.38"
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "@repo/eslint-config": "workspace:*",
    "@types/node": "^22.15.3",
    "typescript": "^5.9.2",
    "vitest": "^2.0.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
