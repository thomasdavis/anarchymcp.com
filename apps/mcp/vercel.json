{
  "version": 2,
  "buildCommand": "cd ../.. && pnpm install && pnpm --filter=@anarchymcp/mcp build",
  "installCommand": "echo 'Skipping default install'",
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/index.js"
    }
  ]
}
